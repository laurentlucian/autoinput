{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister",
    "global-shortcut:allow-is-registered",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-has",
    "store:allow-keys",
    "store:allow-delete"
  ]
}
